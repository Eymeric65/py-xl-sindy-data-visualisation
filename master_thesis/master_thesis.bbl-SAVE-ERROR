      \strng{namehash}{ef9fd3e9ed63dc433e812590fe8b51ed}
      \strng{fullhash}{ef9fd3e9ed63dc433e812590fe8b51ed}
      \strng{bibnamehash}{ef9fd3e9ed63dc433e812590fe8b51ed}
