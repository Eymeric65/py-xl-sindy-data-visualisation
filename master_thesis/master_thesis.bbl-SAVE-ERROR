      \strng{namehash}{ac3fbd0caea38dae790ba92384778f55}
      \strng{fullhash}{ac3fbd0caea38dae790ba92384778f55}
      \strng{bibnamehash}{ac3fbd0caea38dae790ba92384778f55}
      \strng{authorbibnamehash}{ac3fbd0caea38dae790ba92384778f55}
