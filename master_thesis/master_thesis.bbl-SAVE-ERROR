    \entry{purnomoSparseIdentificationLagrangian2023}{article}{}
      \name{author}{2}{}{%
        {{hash=85b81447b7fdb112b9a9e39c551420bd}{%
           family={Purnomo},
           familyi={P\bibinitperiod},
           given={Adam},
